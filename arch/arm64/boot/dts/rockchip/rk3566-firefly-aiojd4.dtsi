#include "rk3568-firefly-port.dtsi"
#include "rk3566-diff.dtsi"

/ {
    vcc_5v_en: vcc-5v-regulator {
       compatible = "regulator-fixed";
       enable-active-high;
       regulator-always-on;
       regulator-boot-on;
       gpio = <&gpio0 RK_PD6 GPIO_ACTIVE_HIGH>;
       pinctrl-names = "default";
       pinctrl-0 = <&vcc_5v_drv>;
       regulator-name = "vcc_5v_en";
    };

    vcc3v3_4g: vcc3v3-4g-regulator {
       compatible = "regulator-fixed";
       enable-active-high;
       regulator-always-on;
       regulator-boot-on;
       gpio = <&gpio0 RK_PD3 GPIO_ACTIVE_HIGH>;
       pinctrl-names = "default";
       pinctrl-0 = <&pcie_4g_drv>;
       regulator-name = "vcc3v3_4g";
    };

    vcc5v0_usb30_en: vcc5v0-usb30-regulator {
       compatible = "regulator-fixed";
       enable-active-high;
       regulator-always-on;
       regulator-boot-on;
       gpio = <&gpio4 RK_PA5 GPIO_ACTIVE_HIGH>;
       pinctrl-names = "default";
       pinctrl-0 = <&vcc5v0_usb30_drv>;
       regulator-name = "vcc5v0_usb30_en";
    };

    vcc_hub_reset: vcc-hub-reset-regulator {
            compatible = "regulator-fixed";
            enable-active-high;
            gpio = <&gpio3 RK_PB0 GPIO_ACTIVE_HIGH>;
            pinctrl-names = "default";
            pinctrl-0 = <&vcc_hub_reset_en>;
            regulator-name = "vcc_hub_reset_en";
            regulator-always-on;
    };

    firefly_leds: leds {
        status = "okay";
        compatible = "gpio-leds";
        pinctrl-names = "default";
        pinctrl-0 =<&leds_gpio>;
        power_led: power {
            label = "firefly:blue:power";
            linux,default-trigger = "ir-power-click";
            default-state = "on";
            gpios = <&gpio4 RK_PB6 GPIO_ACTIVE_HIGH>;
        };

        user_led: user {
            label = "firefly:yellow:user";
            linux,default-trigger = "ir-user-click";
            default-state = "off";
            gpios = <&gpio4 RK_PB0 GPIO_ACTIVE_HIGH>;
        };
        fan_ctl: fan-ctl {
            label = "firefly:fan_ctl:user";
            linux,default-trigger = "ir-user-click";
            default-state = "on";
            gpios = <&gpio4 RK_PA1 GPIO_ACTIVE_HIGH>;
        };
    };
};

/* USB vbus config  */
&vcc5v0_host {
    gpio = <&gpio3 RK_PD2 0>;
    pinctrl-names = "default";
    pinctrl-0 = <&vcc5v0_host_vbus_en>;
};
&vcc5v0_otg {
    gpio = <&gpio3 RK_PD1 0>;
    pinctrl-names = "default";
    pinctrl-0 = <&vcc5v0_otg_vbus_en>;
};

/* WiFi config */
&sdmmc1 {
    max-frequency = <150000000>;
    supports-sdio;
    bus-width = <4>;
    disable-wp;
    cap-sd-highspeed;
    cap-sdio-irq;
    keep-power-in-suspend;
    mmc-pwrseq = <&sdio_pwrseq>;
    non-removable;
    pinctrl-names = "default";
    pinctrl-0 = <&sdmmc1_bus4 &sdmmc1_cmd &sdmmc1_clk>;
    sd-uhs-sdr104;
    rockchip,default-sample-phase = <90>;
    status = "okay";
};

&sdio_pwrseq {
    reset-gpios = <&gpio2 RK_PB1 GPIO_ACTIVE_LOW>;
    status = "okay";
};

&wifi_enable_h {
    rockchip,pins = <2 RK_PB1 RK_FUNC_GPIO &pcfg_pull_none>;
};

// iomux with GPIO2_B2(WIFI,host_wake_irq)
&uart8 {
    status = "disabled";
};

&wireless_wlan {
    wifi_chip_type = "ap6255";
    pinctrl-names = "default";
    pinctrl-0 = <&wifi_host_wake_irq>;
    //WIFI,poweren_gpio = <&gpio3 29 0>;
    WIFI,host_wake_irq = <&gpio2 RK_PB2 0>;
    status = "okay";
};

/* BT config */
&uart1 {
    pinctrl-names = "default";
    pinctrl-0 = <&uart1m0_xfer &uart1m0_ctsn>;
    status = "okay";
};

&wireless_bluetooth {
    uart_rts_gpios = <&gpio2 RK_PB5 GPIO_ACTIVE_LOW>;
    pinctrl-names = "default", "rts_gpio";
    pinctrl-0 = <&uart1m0_rtsn
        &bt_host_wake_gpio
        &bt_poweren
        &bt_host_wake_irq>;
    pinctrl-1 = <&uart1_rts_gpio>;
    BT,reset_gpio    = <&gpio2 RK_PB7 GPIO_ACTIVE_HIGH>;
    BT,wake_gpio     = <&gpio2 RK_PC1 GPIO_ACTIVE_HIGH>;
    BT,wake_host_irq = <&gpio2 RK_PC0 GPIO_ACTIVE_HIGH>;
    status = "okay";
};

&rk809_codec {
    hp-ctl-gpios = <&gpio3 RK_PD0 GPIO_ACTIVE_HIGH>;
    spk-ctl = <&gpio1 RK_PA4 GPIO_ACTIVE_HIGH>;
    io-channels = <&saradc 2>;
    hp-det-adc-value = <200>;
    hp-adc-drift-scope = <200>;
};
&spk_ctl_gpio{
	rockchip,pins = <1 RK_PA4 RK_FUNC_GPIO &pcfg_pull_up>;
};
&can1{
	status = "disabled";
};

&pcie2x1 {
    reset-gpios = <&gpio1 RK_PB2 GPIO_ACTIVE_HIGH>;
    pinctrl-names = "default";
    pinctrl-0 = <&pcie_reset_gpio>;
    /delete-property/ vpcie3v3-supply;
    status = "okay";
};

&sata2 {
    status = "disabled";
};

&its {
    status = "okay";
};

&pwm7 {
    status = "okay";
};

&uart0 {
    status = "okay";
};

&uart3 {
    pinctrl-0 = <&uart3m0_xfer>;
    status = "okay";
};

&uart9 {
    pinctrl-0 = <&uart9m1_xfer>;
    status = "okay";
};

&spi1 {
        status = "okay";
};

&pinctrl {
	vcc_5v {
		vcc_5v_drv: vcc-5v-drv{
			rockchip,pins = <0 RK_PD6 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};
	usb {
		vcc5v0_otg_vbus_en: vcc5v0-vbus-en {
			rockchip,pins = <3 RK_PD1 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		vcc5v0_host_vbus_en: vcc5v0-host-en {
			rockchip,pins = <3 RK_PD2 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

    wireless-wlan {
        wifi_host_wake_irq: wifi-host-wake-irq {
            rockchip,pins = <2 RK_PB2 RK_FUNC_GPIO &pcfg_pull_down>;
        };
    };

    wireless-bluetooth {
        uart1_rts_gpio: uart1-rts-gpio {
            rockchip,pins = <2  RK_PB5 RK_FUNC_GPIO &pcfg_pull_none>;
        };

        bt_host_wake_irq: bt-host-wake-irq {
            rockchip,pins = <2 RK_PC1 RK_FUNC_GPIO &pcfg_pull_down>;
        };
        bt_host_wake_gpio: bt-host-wake-gpio {
            rockchip,pins = <2 RK_PC0 RK_FUNC_GPIO &pcfg_pull_down>;
        };
        bt_poweren: bt-poweren {
            rockchip,pins = <2 RK_PB7 RK_FUNC_GPIO &pcfg_pull_down>;
        };
    };

    pcie {
        pcie_reset_gpio: pcie-reset-gpio {
			rockchip,pins = <1 RK_PB2 RK_FUNC_GPIO &pcfg_pull_none>;
		};
        pcie_4g_drv: pcie-4g-drv {
            rockchip,pins = <0 27 RK_FUNC_GPIO &pcfg_pull_up>;
        };
    };
    usb {
        vcc_hub_reset_en: vcc-hub-reset-en {
            rockchip,pins = <3 RK_PB0 RK_FUNC_GPIO &pcfg_pull_up>;
        };
        vcc5v0_usb30_drv: vcc5v0-usb30-drv {
            rockchip,pins = <4 RK_PA5 RK_FUNC_GPIO &pcfg_pull_up>;
        };
    };

    leds {
        leds_gpio: leds-gpio {
            rockchip,pins =
            /* led_power */
            <4 RK_PB6 RK_FUNC_GPIO &pcfg_pull_none>,
            /* led_user */
            <4 RK_PB0 RK_FUNC_GPIO &pcfg_pull_none>;
        };
    };
};

&gmac1 {
    phy-mode = "rgmii";
    clock_in_out = "input";

    snps,reset-gpio = <&gpio0 RK_PB7 GPIO_ACTIVE_LOW>;
    snps,reset-active-low;
    /* Reset time is 20ms, 100ms for rtl8211f */
    snps,reset-delays-us = <0 20000 100000>;

    assigned-clocks = <&cru SCLK_GMAC1_RX_TX>, <&cru SCLK_GMAC1>;
    assigned-clock-parents = <&cru SCLK_GMAC1_RGMII_SPEED>, <&gmac1_clkin>;

    pinctrl-names = "default";
    pinctrl-0 = <&gmac1m0_miim
             &gmac1m0_tx_bus2
             &gmac1m0_rx_bus2
             &gmac1m0_rgmii_clk
             &gmac1m0_rgmii_bus
             &gmac1m0_clkinout>;

    tx_delay = <0x49>;
    rx_delay = <0x2d>;

    phy-handle = <&rgmii_phy1>;
    status = "okay";
};

&sfc{
    status = "okay";
};

&i2c2 {
    status = "okay";
    pinctrl-0 = <&i2c2m0_xfer>;
    hym8563: hym8563@51 {
                status = "okay";
                compatible = "haoyu,hym8563";
                reg = <0x51>;
                #clock-cells = <0>;
                rtc-irq-gpio = <&gpio0 RK_PD4 IRQ_TYPE_EDGE_FALLING>;
                clock-frequency = <32768>;
                //clock-output-names = "xin32k";
                /* rtc_int is not connected */
        };

};
&i2c5{
    /delete-node/ hym8563@51;
};
&rk809 {
        rtc {
                status = "disabled";
        };
};
&route_hdmi{
    status = "okay";
    connect = <&vp0_out_hdmi>;
};

&dfi {
    status = "disabled";
};

&dmc {
    status = "disabled";
};

&vcc3v3_pcie{
    status="disabled";
};
